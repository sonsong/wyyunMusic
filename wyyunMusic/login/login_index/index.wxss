page, .index_wrap{
    width: 100%;
    height: 100%;
}
.index_wrap .index_bg{
    height: 50%;
}
.index_wrap .index_img{
    height: 100%;
    width: 100%;
    opacity: 0.3; 
}
.index_wrap .index_select{
    height: 35%;
    text-align: center;
}
.index_wrap .index_select .empty{
   height: 20px; 
}
.index_wrap .index_select .btn{
    box-sizing: border-box;
    width: 80%;
    font-size: 16px;
    color:#c62f2f;
    border:1px solid #c62f2f;
    border-radius: 20px;
    height: 35px;
    line-height: 35px;
    margin:0 auto;
    margin-bottom: 20px;
    background: #fff;
}
.index_wrap .index_select .btn:hover{
    background: #c62f2f;
    color:#fff;
}
.index_wrap .index_select .noRegist{
    font-size: 14px;
    text-decoration: underline;
    color:rgb(68, 67, 67);
}
@import '../../common/css/other.wxss';